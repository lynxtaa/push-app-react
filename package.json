{
  "name": "push-app-react",
  "version": "0.1.0",
  "description": "Pushups Scheduler",
  "main": "webpack.config.js",
  "author": "Alex Kondratyuk <lynxtaa@gmail.com>",
  "repository": "https://github.com/lynxtaa/push-app-react.git",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "*.{js,jsx,css,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "compression": "^1.7.2",
    "cross-env": "^5.1.4",
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-hot-loader": "^4.0.1",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.44",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "clean-webpack-plugin": "^0.1.19",
    "connect-history-api-fallback": "^1.5.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.18.0",
    "husky": "^0.15.0-rc.13",
    "idb-keyval": "^3.0.2",
    "koa-connect": "^2.0.1",
    "lint-staged": "^7.0.3",
    "mini-css-extract-plugin": "^0.4.0",
    "offline-plugin": "^4.9.0",
    "prettier": "^1.11.1",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.13",
    "webpack-serve": "^0.3.1"
  },
  "scripts": {
    "start": "webpack-serve config/webpack.config.dev.js --port 8080 --no-reload",
    "build": "webpack --config config/webpack.config.prod.js --profile",
    "lint": "eslint --ext .jsx,.js config controllers models src *.js",
    "lint:watch": "npm run lint -- --watch",
    "serve": "npm run build && npm run server:prod",
    "server:dev": "node server.js",
    "server:prod": "cross-env NODE_ENV=production node server.js",
    "test": "mocha --reporter spec src/**/*.test.js",
    "posttest": "npm run -s lint"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
